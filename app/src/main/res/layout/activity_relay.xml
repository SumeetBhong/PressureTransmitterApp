<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.orion_instruments.www.pressuretransmitterver11.Relay"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView52"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lower Limit"
        android:layout_marginStart="16dp"
        tools:ignore="HardcodedText"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/textView54"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView54"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lower Delay"
        android:layout_marginStart="16dp"
        tools:ignore="HardcodedText"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/textView55"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView55"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView54"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Upper Limit"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="60dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="194dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="60dp"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="59dp"
        android:text="Upper Delay"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="59dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="120dp" />

    <TextView
        android:id="@+id/textView58"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mannual Reset"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        tools:ignore="HardcodedText"
        app:layout_constraintBottom_toBottomOf="@+id/tabRelay"
        android:layout_marginEnd="31dp"
        app:layout_constraintRight_toRightOf="@+id/button9"
        android:layout_marginBottom="6dp"
        android:layout_marginRight="31dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="UPDATE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:ignore="HardcodedText" />


    <TextView
        android:id="@+id/textRelay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:ignore="HardcodedText,MissingConstraints"
        android:layout_marginStart="17dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/button9"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/button9"
        android:layout_marginLeft="17dp" />

    <TabHost
        android:id="@+id/tabRelay"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="13dp"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <HorizontalScrollView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:fillViewport="true"
                android:scrollbars="none"
                tools:ignore="UselessLeaf" />


            <TabWidget
                android:id="@android:id/tabs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <FrameLayout
                android:id="@android:id/tabcontent"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <RelativeLayout
                    android:id="@+id/relay4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <NumberPicker
                        android:id="@+id/numberPicker14"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="18dp"
                        android:layout_marginStart="18dp"
                        android:layout_marginTop="11dp" />

                    <NumberPicker
                        android:id="@+id/numberPicker15"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignBaseline="@+id/numberPicker14"
                        android:layout_alignBottom="@+id/numberPicker14"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="16dp"
                        android:layout_marginRight="16dp" />

                    <NumberPicker
                        android:id="@+id/numberPicker16"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignBaseline="@+id/numberPicker17"
                        android:layout_alignBottom="@+id/numberPicker17"
                        android:layout_alignLeft="@+id/numberPicker15"
                        android:layout_alignStart="@+id/numberPicker15" />

                    <NumberPicker
                        android:id="@+id/numberPicker17"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_above="@+id/switch21"
                        android:layout_alignLeft="@+id/numberPicker14"
                        android:layout_alignStart="@+id/numberPicker14"
                        android:layout_marginBottom="48dp" />

                    <Switch
                        android:id="@+id/switch21"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="68dp"
                        android:layout_marginStart="68dp"
                        android:layout_toEndOf="@+id/numberPicker17"
                        android:layout_toRightOf="@+id/numberPicker17" />


                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relay1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <NumberPicker
                        android:id="@+id/numberPicker3"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="18dp"
                        android:layout_marginStart="18dp"
                        android:layout_marginTop="11dp" />

                    <NumberPicker
                        android:id="@+id/numberPicker2"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignBaseline="@+id/numberPicker3"
                        android:layout_alignBottom="@+id/numberPicker3"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="16dp"
                        android:layout_marginRight="16dp" />

                    <NumberPicker
                        android:id="@+id/numberPicker4"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignBaseline="@+id/numberPicker5"
                        android:layout_alignBottom="@+id/numberPicker5"
                        android:layout_alignLeft="@+id/numberPicker2"
                        android:layout_alignStart="@+id/numberPicker2" />

                    <NumberPicker
                        android:id="@+id/numberPicker5"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignLeft="@+id/numberPicker3"
                        android:layout_alignParentBottom="true"
                        android:layout_alignStart="@+id/numberPicker3"
                        android:layout_marginBottom="75dp" />

                    <Switch
                        android:id="@+id/switch22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="70dp"
                        android:layout_marginStart="70dp"
                        android:layout_toEndOf="@+id/numberPicker5"
                        android:layout_toRightOf="@+id/numberPicker5" />


                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relay2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <NumberPicker
                        android:id="@+id/numberPicker6"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="18dp"
                        android:layout_marginStart="18dp"
                        android:layout_marginTop="11dp" />

                    <NumberPicker
                        android:id="@+id/numberPicker7"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignBaseline="@+id/numberPicker6"
                        android:layout_alignBottom="@+id/numberPicker6"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="16dp"
                        android:layout_marginRight="16dp" />

                    <NumberPicker
                        android:id="@+id/numberPicker8"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignBaseline="@+id/numberPicker9"
                        android:layout_alignBottom="@+id/numberPicker9"
                        android:layout_alignLeft="@+id/numberPicker7"
                        android:layout_alignStart="@+id/numberPicker7" />

                    <NumberPicker
                        android:id="@+id/numberPicker9"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignLeft="@+id/numberPicker6"
                        android:layout_alignParentBottom="true"
                        android:layout_alignStart="@+id/numberPicker6"
                        android:layout_marginBottom="75dp" />

                    <Switch
                        android:id="@+id/switch19"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="72dp"
                        android:layout_marginStart="72dp"
                        android:layout_toEndOf="@+id/numberPicker9"
                        android:layout_toRightOf="@+id/numberPicker9" />


                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relay3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">


                    <NumberPicker
                        android:id="@+id/numberPicker10"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="18dp"
                        android:layout_marginStart="18dp"
                        android:layout_marginTop="11dp" />

                    <NumberPicker
                        android:id="@+id/numberPicker11"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignBaseline="@+id/numberPicker10"
                        android:layout_alignBottom="@+id/numberPicker10"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="16dp"
                        android:layout_marginRight="16dp" />

                    <NumberPicker
                        android:id="@+id/numberPicker12"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignBaseline="@+id/numberPicker13"
                        android:layout_alignBottom="@+id/numberPicker13"
                        android:layout_alignLeft="@+id/numberPicker11"
                        android:layout_alignStart="@+id/numberPicker11" />

                    <NumberPicker
                        android:id="@+id/numberPicker13"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_alignLeft="@+id/numberPicker10"
                        android:layout_alignParentBottom="true"
                        android:layout_alignStart="@+id/numberPicker10"
                        android:layout_marginBottom="75dp" />

                    <Switch
                        android:id="@+id/switch20"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="70dp"
                        android:layout_marginStart="70dp"
                        android:layout_toEndOf="@+id/numberPicker13"
                        android:layout_toRightOf="@+id/numberPicker13" />


                </RelativeLayout>
            </FrameLayout>
        </LinearLayout>
    </TabHost>


</android.support.constraint.ConstraintLayout>
